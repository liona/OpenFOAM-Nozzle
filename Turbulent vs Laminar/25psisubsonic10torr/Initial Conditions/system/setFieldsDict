FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues (volScalarFieldValue p 500);

regions         
	( 
		boxToCell { 
			box (-1 -1 -1) (-0.0014611 1 1); 
			fieldValues ( volScalarFieldValue p 280000 ); 
		} 
	);


